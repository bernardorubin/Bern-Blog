<h1>Sign In</h1>
<%# We're dont have an object to link the form to %>
<%= form_tag sessions_path do%>
  <div>
    <%= label_tag :email %>
    <%= email_field_tag :email %>
  </div>
  <div>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>
  <%= submit_tag 'Sign In' %>
<% end %>
